{
  "servers" : [ {
    "host" : "<%= jmx_host %>",
    "port" : "<%= jmx_port %>",
    <% if host_alias != '' %>
    "alias": "<%= host_alias %>",
    <% end %>
    <% if jmx_username != '' %>
    "username": "<%= jmx_username %>",
    <% end %>
    <% if jmx_password != '' %>
    "password": "<%= jmx_password %>",
    <% end %>
    "queries" : [ {
      "obj" : "<%= objtype %>",
      <% if result_alias != '' %>
      "resultAlias": "<%= result_alias %>",
      <% end %>
      "attr": [ <%= attributes %> ],
      "outputWriters" : [ {
        "@class" : "com.googlecode.jmxtrans.model.output.KeyOutWriter",
        "settings" : {
          "outputFile" : "<%= outputfile %>",
          "maxLogFileSize" : "<%= maxlogfilesize %>",
          "maxLogBackupFiles" : <%= maxlogbackupfiles %>,
          "typeNames" : ["name"]
        }
      } ]
    } ]
    "numQueryThreads" : 2
  } ]
}
