{
  "servers" : [ {
    "host" : "<%= jmx_host %>",
    "port" : "<%= jmx_port %>",
    <% if jmx_username != '' %>
     "username": "<%= jmx_username %>",
     <% end %>
    <% if jmx_password != '' %>
    "password": "<%= jmx_password %>",
    <% end %>
    "queries" : [queries {
      "obj" : "<%= objtype %>",
      "attr": [ <%= attributes %> ],
      "outputWriters" : [ {
        "@class" : "com.googlecode.jmxtrans.model.output.KeyOutWriter",
        "settings" : {
          "outputFile" : "<%= outputfile %>",
          "maxLogFileSize" : "<%= maxlogfilesize %>",
          "maxLogBackupFiles" : <%= maxlogbackupfiles %>,
          "debug" : <%= debug %>,
          "typeNames" : ["name"]
        }
      } ],
    } ],
    "numQueryThreads" : 2
  } ]
}
