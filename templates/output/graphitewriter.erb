{
"servers" : [ {
    "host" : "<%= jmx_host %>",
    "port" : "<%= jmx_port %>",
    <% if jmx_username != '' %>
    "username": "<%= jmx_username %>",
    <% end %>
    <% if jmx_password != '' %>
    "password": "<%= jmx_password %>",
    <% end %>
    "queries" : [ {
      "obj" : "<%= objtype %>",
      "attr": [ <%= attributes %> ],
      "outputWriters" : [ {
        "@class" : "com.googlecode.jmxtrans.model.output.GraphiteWriter",
        "settings": {
          "host" : "<%= graphite_host %>"
          "port" : "<%= graphite_port %>",
          <% if typenames != "" %>
          "typeNames" : [ "<%= typenames %>" ],
          <% end %>
        }
      }]
    }]
}]
}
